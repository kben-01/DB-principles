{
    "$schema": "http://json-schema.org/draft-04/schema",
    "title": "User",
    "description": "A user of the website",
    "type": "object",
    "properties": 
    {
        "name": 
        {
            "description": "The name of the user",
            "type": "string"
        },
        "age": 
        {
            "description": "The age of the user",
            "type": "number",
            "minimum": 16,
            "maximum": 99
        }
    },
    "required": ["name", "age"]
}